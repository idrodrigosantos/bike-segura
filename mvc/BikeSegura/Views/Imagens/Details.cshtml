@model BikeSegura.Models.Imagens

@{
    ViewBag.Title = "Detalhes Imagem";
    Layout = "~/Views/Shared/_Usuario.cshtml";
}

<div class="container conteudo">
    <div class="row">
        <div class="col-sm-6">
            <label><strong>@Html.DisplayNameFor(model => model.Bicicletas.Modelo)</strong></label>
        </div>
        <div class="col-sm-6">
            <label>@Html.DisplayFor(model => model.Bicicletas.Modelo)</label>
        </div>
        <div class="col-sm-6">
            <label><strong>@Html.DisplayNameFor(model => model.Imagem)</strong></label>
        </div>
        <div class="col-sm-6">
            <label>@Html.DisplayFor(model => model.Imagem)</label>
        </div>

        <div class="form-group col-sm-12">
            <div class="botao-salvar">
                <a class="btn btn-success" href="@Url.Action("Index", "Imagens")" role="button">
                    <i class="fas fa-reply"></i>
                    Voltar
                </a>
                <a class="btn btn-primary" href="@Url.Action("Edit", "Imagens", new { id = Model.Id })" role="button">
                    <i class="fas fa-pen"></i>
                    Editar
                </a>
            </div>
        </div>
    </div>
</div>
